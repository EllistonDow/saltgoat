uptime_kuma:
  enabled: true
  base_dir: /opt/saltgoat/docker/uptime-kuma
  data_dir: /opt/saltgoat/docker/uptime-kuma/data
  bind_host: 127.0.0.1
  http_port: 3001
  image: "louislam/uptime-kuma:1"
  environment: {}
  traefik:
    router: "uptime-kuma"
    domain: ""              # 例如 status.example.com
    aliases: []
    entrypoints:
      - "web"
      - "websecure"
    tls:
      enabled: true
      resolver: "saltgoat"
    extra_labels: []        # 追加额外的 Traefik label
